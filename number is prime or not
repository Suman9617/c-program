
#include <stdio.h>
int main()
{
    int a, b, c = 1;
    printf("Enter the value\n");
    scanf("%d", &a);
    if (a == 0 || a == 1)
        printf("number is not prime\n");
    else if (a == 2)
    {

        printf(" number is prime\n");
    }

    else if (a > 2)
    {

        for (b = 2; b < a; b++)
        {
            if (a % b == 0)
            {
                c = 0;
            }
        }
        if (c)
            printf("prime");
        else
            printf(" not prime");
    }
    return 0;
}
